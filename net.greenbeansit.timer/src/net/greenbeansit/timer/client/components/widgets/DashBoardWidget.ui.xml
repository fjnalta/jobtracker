<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui">
	<ui:style>
	.closeButton
	{
		float: left;
	}
	</ui:style>
	<b:Container fluid="true">
		<b:Row>
			<b:Button ui:field="buttonClose" width="30" height="30" type="DANGER" icon="CLOSE"></b:Button>
		</b:Row>
		<b:Row>
			<b:Container fluid="true" ui:field="content"/>
		</b:Row>
	</b:Container>
</ui:UiBinder> 