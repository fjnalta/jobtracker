<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:org.gwtbootstrap3.client.ui" xmlns:a="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="css/Calendar.css" />
	
	<a:ScrollPanel>
		<g:Container>
			<g:Column width="5%" size="XS_12,SM_12,MD_4,LG_1">
				<g:Button ui:field="leftButton" addStyleNames="{style.button-arrow}" />
			</g:Column>
			<g:Column width="90%" size="XS_12,SM_12,MD_4,LG_8">
				<a:FlexTable ui:field="table" addStyleNames="{style.important}" />
			</g:Column>
			<g:Column width="5%" size="XS_12,SM_12,MD_4,LG_1">
				<g:Button ui:field="rightButton" addStyleNames="{style.button-arrow}" />
			</g:Column>
		</g:Container>
	</a:ScrollPanel>

</ui:UiBinder> 