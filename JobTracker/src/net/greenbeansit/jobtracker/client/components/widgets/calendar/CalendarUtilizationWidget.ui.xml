<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:org.gwtbootstrap3.client.ui" xmlns:a="urn:import:com.google.gwt.user.client.ui" xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style src="css/CalendarUtilization.css" />
	
	<h:ClearFix>
		<g:Column size="XS_1,SM_1,MD_1,LG_1">
			<g:Button ui:field="leftButton" addStyleNames="{style.button-arrow} {style.button-left}" />
		</g:Column>
		<g:Column size="XS_10,SM_10,MD_10,LG_10">
			<a:ScrollPanel>
				<a:FlexTable ui:field="table" addStyleNames="{style.important}" />
			</a:ScrollPanel>
		</g:Column>
		<g:Column size="XS_1,SM_1,MD_1,LG_1">
			<g:Button ui:field="rightButton" addStyleNames="{style.button-arrow} {style.button-right}" />
		</g:Column>
	</h:ClearFix>
</ui:UiBinder> 