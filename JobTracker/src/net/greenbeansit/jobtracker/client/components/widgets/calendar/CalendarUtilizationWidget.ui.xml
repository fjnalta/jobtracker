<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:org.gwtbootstrap3.client.ui" xmlns:a="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="css/CalendarUtilization.css" />
	
	<g:Container fluid="true">
		<g:Row>
			<g:Column width="5%" size="XS_1,SM_1,MD_1,LG_1">
				<g:Button ui:field="leftButton" addStyleNames="{style.button-arrow}" />
			</g:Column>

			<g:Column width="90%" size="XS_8,SM_8,MD_8,LG_8">
				<a:ScrollPanel>
					<a:FlexTable ui:field="table" addStyleNames="{style.important}" />
				</a:ScrollPanel>
			</g:Column>

			<g:Column width="5%" size="XS_1,SM_1,MD_1,LG_1">
				<g:Button ui:field="rightButton" addStyleNames="{style.button-arrow}" />
			</g:Column>
		</g:Row>
	</g:Container>

</ui:UiBinder> 