<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	.slider {
		background-color: #669966;
		color: red;
	}
	
	.slider .active {
		background-color: #FF7600;
	}
	
	.NavTabs>li {
		background-color: black;
	}
	</ui:style>
	<h:ClearFix>
		<b:NavTabs justified="true">
			<b:TabListItem active="true" dataTarget="#tabAuslastung"
				text="AUSLASTUNG" />
			<b:TabListItem dataTarget="#tabArbeitszeit" text="ARBEITSZEIT" />
		</b:NavTabs>
		<b:TabContent>
			<b:TabPane active="true" b:id="tabAuslastung">
				<g:HTMLPanel>
					Hier wird die Auslastung angezeigt.
				</g:HTMLPanel>
			</b:TabPane>
			<b:TabPane b:id="tabArbeitszeit">
				<g:HTMLPanel>
					Hier wird die Arbeitszeit angezeigt.
				</g:HTMLPanel>
			</b:TabPane>
		</b:TabContent>
	</h:ClearFix>
</ui:UiBinder> 