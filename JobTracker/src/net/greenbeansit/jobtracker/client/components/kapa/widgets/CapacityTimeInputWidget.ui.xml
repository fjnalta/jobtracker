<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui">
	<ui:style>
	.buttonText {
		min-width: 50px;
		height: 35px;
	}
	
	.button-blue {
		background: #000099
	}
    .buttonBook {
        align: center;
    }
	</ui:style>
	<b:Container fluid="true">
        <b:Column size="XS_6,SM_6,MD_6,LG_6">
            <b:FormLabel>Projektstart</b:FormLabel>
            <b:TextBox readOnly="true" ui:field="projectStart"
                       addStyleNames="{style.buttonText}"></b:TextBox>
        </b:Column>
        <b:Column size="XS_6,SM_6,MD_6,LG_6">
            <b:FormLabel>Projektende</b:FormLabel>
            <b:TextBox readOnly="true" ui:field="projectEnd"
                       addStyleNames="{style.buttonText}"></b:TextBox>
        </b:Column>
        <b:Column offset="XS_8,SM_8,MD_8,LG_8" size="XS_2,SM_2,MD_2,LG_2">
            <b:FormLabel>LÃ¶schen</b:FormLabel>
            <b:Button ui:field="buttonDelete" addStyleNames="{style.buttonBook}"/>
        </b:Column>
        <b:Column size="XS_2,SM_2,MD_2,LG_2">
            <b:FormLabel>Buchen</b:FormLabel>
            <b:Button type="PRIMARY" text="OK" ui:field="buttonBook" addStyleNames="{style.button-blue}" />
        </b:Column>
	</b:Container>
</ui:UiBinder>