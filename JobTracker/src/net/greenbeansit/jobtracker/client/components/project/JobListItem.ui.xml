<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:gwtbootstrap3="urn:import:org.gwtbootstrap3.client.ui">
	<ui:style>
	.container {
		display: inline-block;
	}
	
	.anchor {
		width: 100%;
		display: inline-block;
	}
	</ui:style>
	<h:ClearFix addStyleNames="{style.container}">
		<gwtbootstrap3:Anchor ui:field="anchor" addStyleNames="{style.anchor}"/>
	</h:ClearFix>
</ui:UiBinder> 