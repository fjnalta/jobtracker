<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	.buttonText {
		min-width: 50px;
		height: 35px;
	}
	
	.button-blue {
		background: #000099;
		margin-top: 25px;
	}
    .buttonBook {
        margin-top: 25px;
    }
	</ui:style>
	<b.html:ClearFix>
        <b:Column size="XS_4,SM_4,MD_4,LG_4">
            <b:FormLabel>Projektstart</b:FormLabel>
            <b:TextBox readOnly="true" ui:field="projectStart"
                       addStyleNames="{style.buttonText}"></b:TextBox>
        </b:Column>
        <b:Column size="XS_4,SM_4,MD_4,LG_4">
            <b:FormLabel>Projektende</b:FormLabel>
            <b:TextBox readOnly="true" ui:field="projectEnd"
                       addStyleNames="{style.buttonText}"></b:TextBox>
        </b:Column>
        <b:Column size="XS_2,SM_2,MD_2,LG_2">
            <b:Button ui:field="buttonDelete" addStyleNames="{style.buttonBook}"/>
        </b:Column>
        <b:Column size="XS_2,SM_2,MD_2,LG_2">
            <b:Button type="PRIMARY" text="Buchen" ui:field="buttonBook" addStyleNames="{style.button-blue}" />
        </b:Column>
	</b.html:ClearFix>
</ui:UiBinder>